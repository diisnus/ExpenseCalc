<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="HboxMain" alignment="CENTER" maxHeight="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HboxViewsForItems.ViewForItemsController">
	<children>
		<PieChart fx:id="macroPieChart" maxWidth="300.0">
         <HBox.margin>
            <Insets bottom="20.0" left="40.0" right="20.0" top="20.0" />
         </HBox.margin></PieChart>
		<TableView fx:id="tableView">
			<columns>
				<TableColumn fx:id="nameTableColumn" maxWidth="131.0" minWidth="-1.0" prefWidth="131.0" text="Name" />
				<TableColumn fx:id="valueTableColumn" maxWidth="116.0" minWidth="-1.0" prefWidth="116.0" text="Value" />
			</columns>
         <HBox.margin>
            <Insets bottom="50.0" right="40.0" top="50.0" />
         </HBox.margin>
		</TableView>
	</children>
</HBox>
